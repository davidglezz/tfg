{
  "name": "crawler",
  "version": "1.0.0",
  "description": "Procesa las web en busca de productos y monitoriza los precios",
  "main": "index.js",
  "scripts": {
    "lint": "tslint \"src/**/*.ts\"",
    "lint-fix": "tslint --fix tests/*.ts -t verbose",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "start": "npm run build && node ./build/index.js",
    "start:watch": "npm run build:watch & nodemon ./build/index.js",
    "build": "node_modules/.bin/tsc",
    "build:watch": "npm run build -- -w",
    "start:dev": "ts-node-dev --fast --respawn ./src/index.ts"
  },
  "keywords": [
    "crawler"
  ],
  "author": "David González García",
  "license": "MIT",
  "dependencies": {
    "@types/async": "^2.0.44",
    "@types/dateformat": "^1.0.1",
    "@types/htmlparser2": "^3.7.31",
    "@types/merge2": "^1.1.2",
    "@types/node": "^8.0.45",
    "@types/normalize-url": "^1.9.1",
    "@types/request": "^2.0.6",
    "@types/sax": "^1.0.0",
    "@types/urijs": "^1.15.34",
    "async": "^2.5.0",
    "charset": "^1.0.1",
    "dateformat": "^3.0.2",
    "htmlmetaparser": "^1.2.0",
    "htmlparser2": "^3.9.2",
    "iconv-lite": "^0.4.19",
    "merge2": "^1.2.0",
    "mysql": "^2.15.0",
    "normalize-url": "^1.9.1",
    "popsicle": "^9.1.0",
    "popsicle-limit": "^1.0.0",
    "request": "^2.83.0",
    "typedi": "^0.5.2",
    "typeorm": "^0.1.1",
    "urijs": "^1.19.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.44",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.6.1"
  }
}
